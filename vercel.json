{  
  "functions": {  
    "public/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },  
  "routes": [  
    { "src": "/(.*\\.php)", "dest": "/public/$1" },  
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|webp|svg|ico))", "dest": "/public/$1" },  
    { "src": "/(.*)", "dest": "/public/index.php" }  
  ]  
}